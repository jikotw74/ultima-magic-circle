  },
  "devDependencies": {
    "@types/three": "^0.160.0",
    "@vitest/coverage-v8": "^1.1.0",
    "gh-pages": "^6.1.0",
    "happy-dom": "^12.10.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
